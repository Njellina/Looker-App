<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E2E2E2"
    tools:context=".AdminSDApplicant">

    <EditText
        android:id="@+id/textEmail"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="510dp"
        android:hint="Company Email"
        android:background="@drawable/rectangle"
        android:textColor="@color/black"
        />

    <EditText
        android:id="@+id/textUsername"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="440dp"
        android:hint="Company Name"
        android:background="@drawable/rectangle"
        android:textColor="@color/black"
        />

    <EditText
        android:id="@+id/textPassword"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="190dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="370dp"
        android:hint="Company Password"
        android:background="@drawable/rectangle"
        android:textColor="@color/black"
        />

    <EditText
        android:id="@+id/textPhone"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="260dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="300dp"
        android:hint="Company Phone"
        android:background="@drawable/rectangle"
        android:textColor="@color/black"
        />

    <EditText
        android:id="@+id/textAddress"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="330dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="230dp"
        android:hint="Company Address"
        android:background="@drawable/rectangle"
        android:textColor="@color/black"
        />

    <ImageButton
        android:id="@+id/searchButton"
        android:layout_width="196dp"
        android:layout_height="73dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="7dp"
        android:layout_marginTop="455dp"
        android:layout_marginEnd="207dp"
        android:layout_marginBottom="76dp"
        android:background="#00FFFFFF"
        app:srcCompat="@drawable/searchbutton"
        />

    <ImageButton
        android:id="@+id/deleteButton"
        android:layout_width="196dp"
        android:layout_height="73dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="212dp"
        android:layout_marginTop="455dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="76dp"
        android:background="#00FFFFFF"
        app:srcCompat="@drawable/deletebutton"
        />

</FrameLayout>